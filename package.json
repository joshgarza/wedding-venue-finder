{
  "dependencies": {
    "knex": "^3.1.0",
      "pg": "^8.17.1"
  },
    "devDependencies": {
      "ts-node": "^10.9.2"
    },
    "scripts": {
      "migrate:make": "knex migrate:make -x ts",
      "migrate:latest": "NODE_OPTIONS='--loader ts-node/esm' knex migrate:latest --knexfile knexfile.ts",
      "migrate:rollback": "NODE_OPTIONS='--loader ts-node/esm' knex migrate:rollback --knexfile knexfile.ts",
      "dev": "ts-node src/index.ts",
      "pipeline": "ts-node bin/run.ts",
      "db:shell": "docker compose exec db psql -U postgres -d wedding_venue_finder"
  }
}
