{
  "dependencies": {
    "axios": "^1.13.2",
    "cli-progress": "^3.12.0",
    "knex": "^3.1.0",
    "ollama": "^0.6.3",
    "pg": "^8.17.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "ts-node": "^10.9.2"
  },
  "scripts": {
    "migrate:make": "knex migrate:make -x ts",
    "migrate:latest": "NODE_OPTIONS='ts-node/esm' knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "NODE_OPTIONS='ts-node/esm' knex migrate:rollback --knexfile knexfile.ts",
    "dev": "ts-node src/index.ts",
    "pipeline": "ts-node bin/run.ts",
    "db:shell": "docker compose exec db psql -U postgres -d wedding_venue_finder",
    "test:crawl": "NODE_OPTIONS='ts-node/esm' ts-node bin/test-crawl.ts",
    "test:image": "NODE_OPTIONS='ts-node/esm' ts-node bin/test-image.ts",
    "test:enrichment": "NODE_OPTIONS='ts-node/esm' ts-node bin/test-enrichment.ts"
  }
}
